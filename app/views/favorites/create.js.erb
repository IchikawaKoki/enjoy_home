$("#post_<%= @post.id %> .favorite").html("<%= j(render 'favorites/favorite', post: @post) %>");
$("#ranking").html("<%= j(render 'posts/ranking', ranking: @ranking) %>");
$("#current_user-favorite").html("<%= current_user.favorite_posts.count %>");
$("#my-favorite-posts").html("<%= j(render 'posts/index', posts: current_user.favorite_posts) %>");